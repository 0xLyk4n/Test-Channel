cat bot | grep -Eo '[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}'

Copy all to ip.txt


takedown.sh
#!/bin/bash
for ip in $(cat ~/ip.txt); do curl http://$ip; done;

